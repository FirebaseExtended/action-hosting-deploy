{
  "private": true,
  "name": "firebase-hosting-preview-action",
  "main": "bin/action.min.js",
  "source": "src/index.ts",
  "devDependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^4.0.0",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.21.4",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.15",
    "@types/tmp": "^0.2.0",
    "babel-jest": "^26.6.0",
    "husky": "^4.2.5",
    "jest": "^26.6.0",
    "microbundle": "^0.13.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "tmp": "^0.2.1",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "format:check": "prettier . --list-different",
    "format": "prettier --write .",
